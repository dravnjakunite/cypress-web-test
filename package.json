{
  "name": "portal-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "cy:ci": "cypress run --browser chrome",
    "cy:ci-ct": "CYPRESS_TEST_TYPE=component cypress run --browser chrome --component",
    "cy:ci-e2e": "cypress run --browser chrome",
    "cy:open": "cypress open",
    "cy:open-ct": "CYPRESS_TEST_TYPE=component cypress open --component ",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@react-nexus)/\" --ci --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.6.3",
  "devDependencies": {
    "cypress": "^13.3.0"
  }
}
